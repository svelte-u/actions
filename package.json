{
    "name": "@sveu/actions",
    "version": "0.5.0",
    "packageManager": "pnpm@8.4.0",
    "description": "A collection of svelte actions",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/actions#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/actions.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/actions/issues"
    },
    "keywords": [
        "svelte",
        "actions",
        "svelte-actions"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        ".": {
            "import": "./index.js",
            "types": "./index.d.ts"
        }
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.0",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "bumpp": "^9.1.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.3.0",
        "fs-extra": "^11.1.1",
        "prettier": "^2.8.8",
        "svelte": "^4.0.5",
        "tsup": "^7.1.0",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6",
        "vitest": "^0.33.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@sveu/browser": "^1.0.0",
        "@sveu/shared": "^1.0.1"
    }
}
